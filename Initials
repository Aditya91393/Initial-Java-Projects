public class Initials
{
public static void main (String[] args)
{
System.out.println("     A  SSSSS");
System.out.println("     A  S    ");
System.out.println("     A  SSSS ");
System.out.println("A    A  S");
System.out.println("AAAAAA  S");

// The Complier outputs the following ^^^ exactly how it was written.

}
}
